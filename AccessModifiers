 }
 
 private var instance3 = class3()
 // cannot access // public var instance3 = class3()
 
 
 class areaBase
 {
     var length: Double = 12.34
     var width: Double = 15.31
     
     fileprivate var area: Double
     {
         get
         {
             return length * width
         }
         set
         {
             length += 1
         }
        
     }
 }
 
 var object = areaBase()
 object.area = object.length * object.width
 print("The area is:",object.area)
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
